@model ASP_NET_MVC_Q6.Models.RouteModel
<!DOCTYPE html>

<html>
<head>
    <link href="~/Content/Table.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <table class="fancytable">
        <tr class="headerrow">
            <td>Area</td>
            <td>Controller</td>
            <td>Action</td>
            <td>Category</td>
        </tr>
        <tr class="datarowodd">
            <td>
                @Model.Area
            </td>
            <td>
                @Model.Controller
            </td>
            <td>
                @Model.Action
            </td>
            <td>
                @Model.Category
            </td>
    </table>
</body>
</html>



